# coding-test
# itertools.permutations()



if __name__ == '__main__':
    a = int(input())
    b = int(input())

    print('{0} \n{1}'.format((a // b), (a / b)))
    
## 예시
   [input]
           HACK 2
           
   [output]
           AC
           AH
           AK
           CA
           CH
           CK
           HA
           HC
           HK
           KA
           KC
           KH
